# 环境变量配置示例

# Solana RPC 节点
HELIUS_API_KEY=your_helius_api_key_here
QUICKNODE_RPC=https://your-endpoint.quiknode.pro/token/

# 数据库
DATABASE_URL=postgresql://user:password@localhost:5432/solsniper
SCYLLA_NODES=localhost:9042
REDIS_URL=redis://localhost:6379

# Kafka
KAFKA_BROKERS=localhost:9092

# Jito (MEV 保护)
JITO_BLOCK_ENGINE_URL=https://mainnet.block-engine.jito.wtf
JITO_AUTH_KEYPAIR_PATH=./keys/jito-auth.json

# 钱包
WALLET_PRIVATE_KEY_PATH=./keys/wallet.json

# 日志
RUST_LOG=info
RUST_BACKTRACE=1

# ML 模型
ML_MODEL_PATH=./models
ENABLE_ONLINE_LEARNING=true

# 监控
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
